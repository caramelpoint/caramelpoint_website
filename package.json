{
  "name": "caramelpoint_website",
  "description": "Caramel Point Website",
  "version": "1.0.0",
  "author": "CaramelPoint",
  "dependencies": {
    "gatsby": "2.32.3",
    "gatsby-plugin-manifest": "2.12.0",
    "gatsby-plugin-offline": "3.10.0",
    "gatsby-plugin-postcss": "3.7.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sharp": "2.14.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0"
  },
  "keywords": [
    "caramelpoint",
    "caramel",
    "caramelpoint website"
  ],
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.2.1",
    "autoprefixer": "10.2.4",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.20.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-webpack-plugin": "^2.5.1",
    "gatsby-plugin-eslint": "3.0.0",
    "gh-pages": "^3.1.0",
    "postcss": "8.2.6",
    "prettier": "2.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caramelpoint/caramelpoint_website"
  }
}
